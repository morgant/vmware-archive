<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.makkintosshu.vmware_archive</string>
    <key>Program</key>
    <string>/usr/local/sbin/vmware_archive</string>
    <key>ProgramArguments</key>
    <array>
      <string>vmware_archive</string>
      <string>/Users/USERNAME/Documents/Virtual\ Machines.localized/</string>
      <string>/var/backups/</string>
    </array>
    <key>StartCalendarInterval</key>
    <dict>
      <key>Day</key>
      <integer>0</integer>
      <key>Hour</key>
      <integer>0</integer>
      <key>Minute</key>
      <integer>0</integer>
    </dict>
    <key>ProcessType</key>
    <string>Background</string>
  </dict>
</plist>
